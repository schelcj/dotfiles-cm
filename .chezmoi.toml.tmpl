{{- $email  := promptString "email" -}}
{{- $lat    := promptString "Latitude" -}}
{{- $lon    := promptString "Longitude" -}}
{{- $indent := promptString "Perl Indent Columns" -}}
[data]
  email = "{{ $email }}"
  lat = "{{ $lat }}"
  lon = "{{ $lon }}"

  [data.perl]
    column_indent = "{{ $indent }}"
